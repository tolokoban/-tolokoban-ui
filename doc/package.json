{
    "name": "doc",
    "version": "0.1.0",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "homepage": "./",
    "dependencies": {
        "@mdx-js/loader": "^2.3.0",
        "@tolokoban/ui": "file:../lib",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^8.0.7"
    },
    "scripts": {
        "pregenerate": "tsc -p scripts/tsconfig.json",
        "generate": "node scripts/dist/prebuild.js && build-react-routes src/app &",
        "prestart": "npm run generate",
        "start": "webpack serve --mode=development",
        "build": "webpack --mode=production",
        "cc": "clear && webpack --mode=development --stats-error-details",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build/",
        "lint": "eslint -c .eslintrc.yml 'src/**/*.ts' 'src/**/*.tsx'",
        "format": "prettier --write ./src/",
        "test": "jest"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.7",
        "@types/react": "^18.2.18",
        "@types/react-dom": "^18.2.7",
        "babel-jest": "^29.6.2",
        "babel-loader": "^9.1.3",
        "build-react-routes": "^0.3.0",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-jsdoc": "^46.4.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-react": "^7.33.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unicorn": "^48.0.1",
        "file-loader": "^6.2.0",
        "gh-pages": "^5.0.0",
        "html-webpack-plugin": "^5.5.3",
        "jest": "^29.6.2",
        "json-loader": "^0.5.7",
        "markdown-to-jsx": "^7.3.1",
        "mini-css-extract-plugin": "^2.7.6",
        "prettier": "3.0.1",
        "style-loader": "^3.3.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-morph": "^19.0.0",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "typescript": "^5.1.6",
        "url-loader": "^4.1.1",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "workbox-webpack-plugin": "^7.0.0",
        "yaml-loader": "^0.8.0"
    },
    "port": 61749
}