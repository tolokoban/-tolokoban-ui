{
    "name": "doc",
    "version": "0.1.0",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "homepage": "./",
    "dependencies": {
        "@mdx-js/loader": "^2.3.0",
        "@tolokoban/ui": "file:../lib",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "scripts": {
        "generate": "tsc -p scripts/tsconfig.json && node scripts/dist/prebuild.js",
        "prestart": "npm link @tolokoban/ui",
        "start": "webpack serve --mode=development",
        "build": "webpack --mode=production",
        "cc": "clear && webpack --mode=development --stats-error-details",
        "deploy": "gh-pages -d build/",
        "lint": "eslint -c .eslintrc.yml 'src/**/*.ts' 'src/**/*.tsx'",
        "format": "prettier --write ./src/",
        "test": "jest"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "@types/node": "^18.15.1",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "babel-jest": "^29.5.0",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.3",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^40.0.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unicorn": "^46.0.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^29.5.0",
        "json-loader": "^0.5.7",
        "markdown-to-jsx": "^7.1.9",
        "mini-css-extract-plugin": "^2.7.4",
        "prettier": "2.8.4",
        "style-loader": "^3.3.2",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-morph": "^17.0.1",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "typescript": "^4.9.5",
        "url-loader": "^4.1.1",
        "webpack": "^5.76.2",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.12.0",
        "workbox-webpack-plugin": "^6.5.4",
        "yaml-loader": "^0.8.0"
    },
    "port": 61749
}
