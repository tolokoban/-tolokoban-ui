{
    "name": "@tolokoban/ui",
    "version": "0.21.5",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "homepage": "https://tolokoban.github.io/ui",
    "dependencies": {
        "@tolokoban/ui": "file:../",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-markdown": "^9.0.1",
        "react-syntax-highlighter": "^15.6.1"
    },
    "scripts": {
        "pregenerate": "tsc -p scripts/tsconfig.json",
        "generate": "node scripts/dist/prebuild.js && build-react-routes src/app &",
        "prestart": "npm run generate",
        "start": "webpack serve --mode=development",
        "build": "webpack --mode=production",
        "cc": "clear && webpack --mode=development --stats-error-details",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build/",
        "lint": "eslint -c .eslintrc.yml 'src/**/*.ts' 'src/**/*.tsx'",
        "format": "prettier --write ./src/",
        "test": "jest"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@mdx-js/loader": "^3.1.0",
        "@types/jest": "^29.5.14",
        "@types/mdx": "^2.0.13",
        "@types/node": "^22.8.6",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/react-syntax-highlighter": "^15.5.13",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.2.1",
        "build-react-routes": "^0.10.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.11.0",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.4.3",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-unicorn": "^56.0.0",
        "file-loader": "^6.2.0",
        "gh-pages": "^6.2.0",
        "html-webpack-plugin": "^5.6.3",
        "jest": "^29.7.0",
        "json-loader": "^0.5.7",
        "markdown-to-jsx": "^7.5.0",
        "mini-css-extract-plugin": "^2.9.2",
        "prettier": "3.3.3",
        "style-loader": "^3.3.4",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-morph": "^24.0.0",
        "ts-node": "^10.9.2",
        "tslint": "^6.1.3",
        "typescript": "^5.6.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.96.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0",
        "workbox-webpack-plugin": "^7.3.0",
        "yaml-loader": "^0.8.1"
    },
    "port": 61749,
    "description": "React components with theme"
}