{
    "name": "@tolokoban/ui",
    "version": "0.21.1",
    "private": false,
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": [
        "*.css"
    ],
    "scripts": {
        "generate": "node scripts/prebuild/prebuild.mjs",
        "prepack": "npm run build",
        "prebuild": "rm -rf dist/* && npm run generate && npm run doc",
        "doc": "typedoc --out ../doc/public/api",
        "build": "build-typescript-library .",
        "postbuild": "npm run doc",
        "lint": "eslint 'src/**/*.ts'",
        "start": "build-typescript-library --watch -runBefore generate --runAfter doc .",
        "test": "jest",
        "test:watch": "jest --watch",
        "prepub": "npm run build",
        "pub": "npm publish --access public",
        "prestart": "npm run generate"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@eslint/compat": "^1.2.2",
        "@types/node": "^22.8.6",
        "@types/react": "^18.3.12",
        "acorn": "^8.14.0",
        "babel-loader": "^9.2.1",
        "build-typescript-library": "^0.7.4",
        "chalk": "^5.3.0",
        "chokidar": "^4.0.1",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.4.3",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-unicorn": "^55.0.0",
        "ts-loader": "^9.5.1",
        "ts-patch": "^3.2.1",
        "typedoc": "^0.26.10",
        "typescript": "^5.6.3",
        "typescript-plugin-css-modules": "^5.1.0",
        "typescript-transform-paths": "^3.5.1"
    },
    "dependencies": {
        "json5": "^2.2.3",
        "tslib": "^2.8.1"
    },
    "peerDependencies": {
        "react": "^18.3.1"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "description": "React components with theme",
    "homepage": "https://tolokoban.github.io/ui"
}